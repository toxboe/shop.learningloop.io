{%- liquid
  assign options = section.settings

  assign style = options.style
  assign color_scheme = options.color_scheme
  assign section_spacing_top = options.section_spacing_top
  assign section_spacing_bottom = options.section_spacing_bottom
  assign container_size = options.container_size
  assign enable_background_around_container = options.enable_background_around_container
  assign expand_first_collapsible_row = options.expand_first_collapsible_row

  assign first_block = section.blocks | first
-%}

{% capture css %}
  {% style %}
    .section-{{ section.id }} {
      --gsc-section-spacing-top: {{ section_spacing_top }}px;
      --gsc-section-spacing-bottom: {{ section_spacing_bottom }}px;
    }

    {% for block in section.blocks %}
        .block-{{block.id}}{
            --gsc-image-width: {{ block.settings.image_width }}%;
        }
    {% endfor %}
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

<product-details class='product-details section-{{ section.id }} {% if enable_background_around_container %} section color-{{ color_scheme }} {% endif %}'>
  <div class='section-spacing container container--{{ container_size }}'>
    <div class='product-details__container {% unless enable_background_around_container %} product-details__container--with-aside-spacing section color-{{ color_scheme }}  {% endunless %}'>
      {%- if style == 'tabs' -%}
        <tabs-component data-selected-tab-id='1'>
          <div class='product-details__tab-names'>
            {%- for block in section.blocks -%}
              {% assign custom_icon_image = block.settings.custom_icon_image %}
              {% assign icon = block.settings.icon %}

              <div
                data-tabs-name
                class='tab-name product-details__tab-name {% if forloop.index == 1 %}selected{% endif %}'
                data-id='{{ forloop.index }}'
                tabindex='0'
              >
                {%- if custom_icon_image != blank -%}
                  <img
                    {% render 'image-attributes',
                      image: custom_icon_image,
                      size: 'small',
                      class: 'product-details__tab-name-icon'
                    %}
                  >
                {%- elsif icon != 'none' -%}
                  {% render 'icon-pack', icon: icon, class: 'product-details__tab-name-icon' %}
                {%- endif -%}
                {{- block.settings.title }}
              </div>
            {%- endfor -%}
          </div>

          <carousel-component
            class='carousel product-details__tab-name-carousel'
            data-contain-scroll='trimSnaps'
            data-section-id='{{ section.id }}'
            with-stop-on-hover
            data-drag-free
            data-draggable
            data-align='start'
            data-axis='x'
          >
            <div
              class='carousel__viewport carousel__viewport--overflow embla'
              data-carousel-viewport
            >
              <div class='carousel__container' data-carousel-container>
                {%- for block in section.blocks -%}
                  {% assign custom_icon_image = block.settings.custom_icon_image %}
                  {% assign icon = block.settings.icon %}

                  <div class='product-details__tab-name-carousel-slide carousel__slide' data-carousel-item>
                    <div
                      data-tabs-name
                      class='tab-name product-details__tab-name {% if forloop.index == 1 %}selected{% endif %}'
                      data-id='{{ forloop.index }}'
                      tabindex='0'
                    >
                      {%- if custom_icon_image != blank -%}
                        <img
                          {% render 'image-attributes',
                            image: custom_icon_image,
                            size: 'small',
                            class: 'product-details__tab-name-icon'
                          %}
                        >
                      {%- elsif icon != 'none' -%}
                        {% render 'icon-pack', icon: icon, class: 'product-details__tab-name-icon' %}
                      {%- endif -%}
                      {{ block.settings.title }}
                    </div>
                  </div>
                {%- endfor -%}
              </div>
            </div>
          </carousel-component>

          <div class='tabs product-details__tabs' data-tabs-container>
            <template data-tabs-elements-template>
              {%- for block in section.blocks offset: 1 -%}
                {% assign custom_icon_image = block.settings.custom_icon_image %}
                {% assign icon = block.settings.icon %}

                {%- case block.type -%}
                  {%- when 'description' -%}
                    <tab-with-dynamic-height
                      class='tab product-details__tab block-{{block.id}}'
                      aria-hidden='true'
                      data-id='{{ forloop.index | plus: 1 }}'
                    >
                      <div
                        data-tabs-element-content
                        class='rte product-details__rte  body-font-weight-from-global-settings'
                        {{ block.shopify_attributes }}
                      >
                        {{ product.description }}
                      </div>
                    </tab-with-dynamic-height>
                  {%- when 'collapsible_row' -%}
                    <tab-with-dynamic-height
                      class='tab product-details__tab block-{{block.id}}'
                      aria-hidden='true'
                      data-id='{{ forloop.index | plus: 1 }}'
                    >
                      {%- assign image = block.settings.image -%}
                      {%- assign video = block.settings.video -%}
                      {%- assign page = block.settings.page -%}
                      {%- assign text = block.settings.text -%}

                      <div
                        class='product-details__content-tab-content body-font-weight-from-global-settings'
                        data-tabs-element-content
                        {{ block.shopify_attributes }}
                      >
                        {% if text != blank %}
                          <div class='rte product-details__rte'>
                            {{ text }}
                          </div>
                        {% endif %}

                        {% if page != blank %}
                          <div class='rte rte--page product-details__rte'>
                            {{ page.content }}
                          </div>
                        {% endif %}

                        {% if video != blank %}
                          {% render 'video-with-alt',
                            video: video,
                            class: 'image product-details__image',
                            autoplay: true,
                            loop: true,
                            muted: true,
                            controls: false,
                            loading: 'eager'
                          %}
                        {% elsif image != blank %}
                          <img
                            {% render 'image-attributes', image: image, class: 'image product-details__image' %}
                          >
                        {% endif %}
                      </div>
                    </tab-with-dynamic-height>
                  {%- when 'custom_liquid' -%}
                    <tab-with-dynamic-height
                      class='tab product-details__tab block-{{block.id}}'
                      aria-hidden='true'
                      data-id='{{ forloop.index | plus: 1 }}'
                    >
                      <div
                        class='rte product-details__rte  body-font-weight-from-global-settings'
                        data-tabs-element-content
                        {{ block.shopify_attributes }}
                      >
                        {{ block.settings.custom_liquid }}
                      </div>
                    </tab-with-dynamic-height>
                {%- endcase -%}
              {%- endfor -%}
            </template>

            {%- case first_block.type -%}
                {% assign custom_icon_image = first_block.settings.custom_icon_image %}
                {% assign icon = first_block.settings.icon %}

              {%- when 'description' -%}
                <tab-with-dynamic-height
                  class='tab product-details__tab block-{{first_block.id}}'
                  aria-hidden='false'
                  data-id='1'
                >
                  <div
                    class='rte product-details__rte  body-font-weight-from-global-settings'
                    data-tabs-element-content
                    {{ first_block.shopify_attributes }}
                  >
                    {{ product.description }}
                  </div>
                </tab-with-dynamic-height>
              {%- when 'collapsible_row' -%}
                <tab-with-dynamic-height
                  class='tab product-details__tab block-{{first_block.id}}'
                  aria-hidden='false'
                  data-id='1'
                >
                  {%- assign image = first_block.settings.image -%}
                  {%- assign video = first_block.settings.video -%}
                  {%- assign page = first_block.settings.page -%}
                  {%- assign text = first_block.settings.text -%}

                  <div
                    class='product-details__content-tab-content body-font-weight-from-global-settings'
                    data-tabs-element-content
                    {{ first_block.shopify_attributes }}
                  >
                    {% if text != blank %}
                      <div class='rte product-details__rte'>
                        {{ text }}
                      </div>
                    {% endif %}

                    {% if page != blank %}
                      <div class='rte rte--page product-details__rte'>
                        {{ page.content }}
                      </div>
                    {% endif %}

                    {%- if image != blank or video != blank -%}
                      {% if video != blank %}
                        {% render 'video-with-alt',
                          video: video,
                          class: 'image product-details__image',
                          autoplay: true,
                          loop: true,
                          muted: true,
                          controls: false,
                          loading: 'eager'
                        %}
                      {% elsif image != blank %}
                        <img
                          {% render 'image-attributes', image: image, class: 'image product-details__image' %}
                        >
                      {% endif %}
                    {%- endif -%}
                  </div>
                </tab-with-dynamic-height>
              {%- when 'custom_liquid' -%}
                <tab-with-dynamic-height
                  class='tab product-details__tab block-{{ first_block.id }}'
                  aria-hidden='false'
                  data-id='1'
                >
                  <div
                    class='rte product-details__rte  body-font-weight-from-global-settings'
                    data-tabs-element-content
                    {{ first_block.shopify_attributes }}
                  >
                    {{ first_block.settings.custom_liquid }}
                  </div>
                </tab-with-dynamic-height>
            {%- endcase -%}
          </div>
        </tabs-component>
      {%- else -%}
        {%- for block in section.blocks -%}
          {% assign custom_icon_image = block.settings.custom_icon_image %}
          {% assign icon = block.settings.icon %}

          {%- case block.type -%}
            {%- when 'description' -%}
              <accordeon-component
                class='accordeon-plus {% unless forloop.last %} accordeon-plus--bottom-bordered{% endunless %} {% unless forloop.first %} accordeon-plus--top-bordered{% endunless %} block-{{block.id}}'
                data-aria-expanded='{% if expand_first_collapsible_row and forloop.index == 1 %}true{% else %}false{% endif %}'
                data-section-id='{{ section.id }}'
                data-block-id='{{ block.id }}'
                {{ block.shopify_attributes }}
              >
                <accordeon-btn
                  class='accordeon-plus__btn accordeon-plus__btn--small product-details__accordeon-btn'
                  tabindex='0'
                >
                  {%- if custom_icon_image != blank -%}
                    <img
                      {% render 'image-attributes',
                        image: custom_icon_image,
                        size: 'small',
                        class: 'accordeon-plus__btn-icon'
                      %}
                    >
                  {%- elsif icon != 'none' -%}
                    {% render 'icon-pack', icon: icon, class: 'accordeon-plus__btn-icon' %}
                  {%- endif -%}
                  {{ block.settings.title }}
                  {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                </accordeon-btn>
                <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                  <div class='accordeon-plus__content product-details__accordeon-content' data-accordeon-content>
                    <div class='rte product-details__rte  body-font-weight-from-global-settings'>
                      {{ product.description }}
                    </div>
                  </div>
                </div>
              </accordeon-component>
            {%- when 'collapsible_row' -%}
              <accordeon-component
                class='accordeon-plus {% unless forloop.last %} accordeon-plus--bottom-bordered{% endunless %} {% unless forloop.first %} accordeon-plus--top-bordered{% endunless %} block-{{block.id}}'
                data-aria-expanded='{% if expand_first_collapsible_row and forloop.index == 1 %}true{% else %}false{% endif %}'
                data-section-id='{{ section.id }}'
                data-block-id='{{ block.id }}'
                {{ block.shopify_attributes }}
              >
                <accordeon-btn
                  class='accordeon-plus__btn accordeon-plus__btn--small product-details__accordeon-btn'
                  tabindex='0'
                >
                  {%- if custom_icon_image != blank -%}
                    <img
                      {% render 'image-attributes',
                        image: custom_icon_image,
                        size: 'small',
                        class: 'accordeon-plus__btn-icon'
                      %}
                    >
                  {%- elsif icon != 'none' -%}
                    {% render 'icon-pack', icon: icon, class: 'accordeon-plus__btn-icon' %}
                  {%- endif -%}
                  {{ block.settings.title }}
                  {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                </accordeon-btn>
                <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                  <div class='accordeon-plus__content product-details__accordeon-content' data-accordeon-content>
                    {%- assign image = block.settings.image -%}
                    {%- assign video = block.settings.video -%}
                    {%- assign page = block.settings.page -%}
                    {%- assign text = block.settings.text -%}

                    <div class='product-details__content-tab-content body-font-weight-from-global-settings'>
                      {% if text != blank %}
                        <div class='rte product-details__rte'>
                          {{ text }}
                        </div>
                      {% endif %}

                      {% if page != blank %}
                        <div class='rte rte--page product-details__rte'>
                          {{ page.content }}
                        </div>
                      {% endif %}

                      {% if video != blank %}
                        {% render 'video-with-alt',
                          video: video,
                          class: 'image product-details__image',
                          autoplay: true,
                          loop: true,
                          muted: true,
                          controls: false,
                          loading: 'lazy'
                        %}
                      {% elsif image != blank %}
                        <img
                          {% render 'image-attributes', image: image, class: 'image product-details__image' %}
                        >
                      {% endif %}
                    </div>
                  </div>
                </div>
              </accordeon-component>
            {%- when 'custom_liquid' -%}
              <accordeon-component
                class='accordeon-plus {% unless forloop.last %} accordeon-plus--bottom-bordered{% endunless %} {% unless forloop.first %} accordeon-plus--top-bordered{% endunless %} block-{{block.id}}'
                data-aria-expanded='{% if expand_first_collapsible_row and forloop.index == 1 %}true{% else %}false{% endif %}'
                data-section-id='{{ section.id }}'
                data-block-id='{{ block.id }}'
                {{ block.shopify_attributes }}
              >
                <accordeon-btn
                  class='accordeon-plus__btn accordeon-plus__btn--small product-details__accordeon-btn'
                  tabindex='0'
                >
                  {%- if custom_icon_image != blank -%}
                    <img
                      {% render 'image-attributes',
                        image: custom_icon_image,
                        size: 'small',
                        class: 'accordeon-plus__btn-icon'
                      %}
                    >
                  {%- elsif icon != 'none' -%}
                    {% render 'icon-pack', icon: icon, class: 'accordeon-plus__btn-icon' %}
                  {%- endif -%}
                  {{ block.settings.title }}
                  {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                </accordeon-btn>
                <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                  <div class='accordeon-plus__content product-details__accordeon-content' data-accordeon-content>
                    <div class='rte product-details__rte  body-font-weight-from-global-settings'>
                      {{ block.settings.custom_liquid }}
                    </div>
                  </div>
                </div>
              </accordeon-component>
          {%- endcase -%}
        {%- endfor -%}
      {%- endif -%}
    </div>
  </div>
</product-details>

{% render 'save-section-data-in-global-scope', section: section %}

{% schema %}
{
  "name": "t:sections.product_details.name",
  "tag": "section",
  "enabled_on": {
    "templates": ["product"]
  },
  "settings": [
    {
      "type": "select",
      "id": "color_scheme",
      "options": [
        {
          "value": "default",
          "label": "t:sections.general_section_settings.color_scheme_option_default_label"
        },
        {
          "value": "primary",
          "label": "t:sections.general_section_settings.color_scheme_option_primary_label"
        },
        {
          "value": "secondary",
          "label": "t:sections.general_section_settings.color_scheme_option_secondary_label"
        },
        {
          "value": "custom-1",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_1_label"
        },
        {
          "value": "custom-2",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_2_label"
        },
        {
          "value": "custom-3",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_3_label"
        },
        {
          "value": "white",
          "label": "t:sections.general_section_settings.color_scheme_option_white_text_label"
        },
        {
          "value": "black",
          "label": "t:sections.general_section_settings.color_scheme_option_black_text_label"
        }
      ],
      "default": "custom-1",
      "label": "t:sections.general_section_settings.color_scheme_label",
      "info": "t:sections.general_section_settings.color_scheme_tip"
    },
    {
      "type": "checkbox",
      "id": "enable_background_around_container",
      "default": true,
      "label": "t:sections.general_section_settings.enable_background_around_container_label"
    },
    {
      "type": "select",
      "id": "container_size",
      "options": [
        {
          "value": "page-width",
          "label": "t:sections.general_section_settings.container_size.page_width_option"
        },
        {
          "value": "large",
          "label": "t:sections.general_section_settings.container_size.large_width_option"
        },
        {
          "value": "medium",
          "label": "t:sections.general_section_settings.container_size.medium_width_option"
        },
        {
          "value": "small",
          "label": "t:sections.general_section_settings.container_size.small_width_option"
        },
        {
          "value": "xsmall",
          "label": "t:sections.general_section_settings.container_size.extra_small_width_option"
        }
      ],
      "default": "large",
      "label": "t:sections.general_section_settings.container_size.label"
    },
    {
      "type": "range",
      "id": "section_spacing_top",
      "min": 0,
      "max": 100,
      "step": 1,
      "default": 0,
      "unit": "t:sections.general_section_settings.section_spacing_top.unit",
      "label": "t:sections.general_section_settings.section_spacing_top.label"
    },
    {
      "type": "range",
      "id": "section_spacing_bottom",
      "min": 0,
      "max": 100,
      "step": 1,
      "default": 0,
      "unit": "t:sections.general_section_settings.section_spacing_bottom.unit",
      "label": "t:sections.general_section_settings.section_spacing_bottom.label"
    },
    {
      "type": "select",
      "id": "style",
      "options": [
        {
          "value": "tabs",
          "label": "t:sections.product_details.style_option_tabs_label"
        },
        {
          "value": "collapsible-rows",
          "label": "t:sections.product_details.style_option_collapsible_rows_label"
        }
      ],
      "default": "collapsible-rows",
      "label": "t:sections.product_details.style_label"
    },
    {
      "type": "checkbox",
      "default": false,
      "id": "expand_first_collapsible_row",
      "label": "t:sections.product_details.expand_first_collapsible_row.label",
      "info": "t:sections.product_details.expand_first_collapsible_row.info"
    }
  ],
  "blocks": [
    {
      "type": "description",
      "name": "t:sections.product_details.description_block_name",
      "limit": 1,
      "settings": [
        {
          "type": "text",
          "id": "title",
          "default": "Description",
          "label": "t:sections.product_details.description_block_title_label"
        },
        {
          "type": "select",
          "id": "icon",
          "options": [
            {
              "value": "none",
              "label": "t:sections.general_section_settings.icon.none_icon_label"
            },
            {
              "value": "settings",
              "label": "t:sections.general_section_settings.icon.general_group.settings_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "search",
              "label": "t:sections.general_section_settings.icon.general_group.search_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "eye",
              "label": "t:sections.general_section_settings.icon.general_group.eye_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "eye-slash",
              "label": "t:sections.general_section_settings.icon.general_group.eye_slash_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "user",
              "label": "t:sections.general_section_settings.icon.general_group.user_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "love-outline",
              "label": "t:sections.general_section_settings.icon.general_group.love_outline_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "love-solid",
              "label": "t:sections.general_section_settings.icon.general_group.love_solid_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "like",
              "label": "t:sections.general_section_settings.icon.general_group.like_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "dislike",
              "label": "t:sections.general_section_settings.icon.general_group.dislike_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "lamp",
              "label": "t:sections.general_section_settings.icon.general_group.lamp_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "star-outline",
              "label": "t:sections.general_section_settings.icon.general_group.star_outline_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "star-solid",
              "label": "t:sections.general_section_settings.icon.general_group.star_solid_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "trash",
              "label": "t:sections.general_section_settings.icon.general_group.trash_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "document",
              "label": "t:sections.general_section_settings.icon.general_group.document_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "copy",
              "label": "t:sections.general_section_settings.icon.general_group.copy_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "share",
              "label": "t:sections.general_section_settings.icon.general_group.share_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "plus",
              "label": "t:sections.general_section_settings.icon.general_group.plus_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "minus",
              "label": "t:sections.general_section_settings.icon.general_group.minus_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "checkmark",
              "label": "t:sections.general_section_settings.icon.general_group.checkmark_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "arrow-right",
              "label": "t:sections.general_section_settings.icon.general_group.arrow_right_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "arrow-left",
              "label": "t:sections.general_section_settings.icon.general_group.arrow_left_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
            },
            {
              "value": "undo",
              "label": "t:sections.general_section_settings.icon.general_group.undo_icon_label",
              "group": "t:sections.general_section_settings.icon.general_group.label"
