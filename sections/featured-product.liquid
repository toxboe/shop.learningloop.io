{%- liquid
  assign section_spacing_top = section.settings.section_spacing_top
  assign section_spacing_bottom = section.settings.section_spacing_bottom
  assign color_scheme = section.settings.color_scheme
  assign container_size = section.settings.container_size

  assign product_media = product.media | default: (1..12)
  assign block_layout = section.settings.layout

  assign sectionIdWithFeaturedProductPrefix = section.id | append: '---FeaturedProduct'

  assign option_to_track = settings.product_media_filtering_option

  assign option = product.options_by_name[settings.product_media_filtering_option]

  assign options = option.values | join: ','
  assign option_position = 'option' | append: option.position
-%}

{% capture css %}
  {% style %}
    .section-{{section.id}} {
        --gsc-section-spacing-top: {{ section_spacing_top }}px;
        --gsc-section-spacing-bottom: {{ section_spacing_bottom }}px;
        --gsc-desktop-space-between-media: {{ section.settings.desktop_space_between_media }}px;
        --gsc-thumbnail-width: {{ section.settings.thumbnails_size }}px;
    }
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

{% if settings.product_media_filtering_option != blank and section.settings.show_only_selected_variants == true %}
  <product-content
    data-option-to-track='{{ option_to_track }}'
    data-options='{{ options }}'
    data-option-position='{{ option_position }}'
    class='product section section-{{ section.id }} color-{{ color_scheme }} section-spacing {% if block_layout contains 'column' %} product--with-desktop-media-grid {% endif %} {% if block_layout == 'thumbnails-left' or block_layout == 'thumbnails-right' or block_layout == 'thumbnails-bottom' %}  {% if container_size == 'page-width' %} product--with-page-width-container-and-desktop-media-carousel {% else %} product--with-desktop-media-carousel {% endif %}  {% endif %}'
  >
    <div class='container container--{{ container_size }} product__container'>
      {% render 'product',
        product: section.settings.product,
        section: section,
        sectionId: sectionIdWithFeaturedProductPrefix,
        block_layout: block_layout
      %}
    </div>
  </product-content>

{% else %}
  <div
    class='product section section-{{ section.id }} color-{{ color_scheme }} section-spacing {% if block_layout contains 'column' %} product--with-desktop-media-grid {% endif %} {% if block_layout == 'thumbnails-left' or block_layout == 'thumbnails-right' or block_layout == 'thumbnails-bottom' %}  {% if container_size == 'page-width' %} product--with-page-width-container-and-desktop-media-carousel {% else %} product--with-desktop-media-carousel {% endif %}  {% endif %}'
  >
    <div class='container container--{{ container_size }} product__container'>
      {% render 'product',
        product: section.settings.product,
        section: section,
        sectionId: sectionIdWithFeaturedProductPrefix,
        block_layout: block_layout
      %}
    </div>
  </div>
{% endif %}

{% render 'save-section-data-in-global-scope', section: section %}

