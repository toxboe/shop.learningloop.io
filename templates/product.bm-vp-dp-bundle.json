    "a9b7acc2-f81e-4056-88b3-cdf6ae89c7a7": {
      "type": "rich-text",
      "blocks": {
        "template--15412947091490__a9b7acc2-f81e-4056-88b3-cdf6ae89c7a7-icon-1": {
          "type": "icon",
          "settings": {
            "icon": "none",
            "icon_size": 48
