          }
        },
        "quote_pMCw3m": {
          "type": "quote",
          "settings": {
            "text": "\"Loving my persuasive patterns cards. Really useful inspirational tool.\"",
            "author_name": "Paul Boag, Boagworks",
            "author_image": "shopify://shop_images/paul-boag.png",
            "rating": 5,
